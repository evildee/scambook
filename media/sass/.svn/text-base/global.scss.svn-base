filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#f2f2f2',GradientType=0 ); /* IE6-8 */
}

/* Buttons */

@mixin gradi($gradient) {
	@if $gradient == white {
		background-color: rgb(255,255,255);
		@include filter-gradient(#ffffff, #f2f2f2, vertical);
		$experimental-support-for-svg: true;
		@include background-image(linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(242,242,242,1) 100%));
	}
	@else if $gradient == magenta {
		background-color: rgb(237,85,152);
		@include filter-gradient(#ed5598, #b9026d, vertical);
		$experimental-support-for-svg: true;
		@include background-image(linear-gradient(top,  rgba(237,85,152,1) 0%,rgba(185,2,109,1) 100%));
	}
	@else if $gradient == magenta-dark {
		background-color: rgb(237,85,152);
		@include filter-gradient(#ed5598, #93024b, vertical);
		$experimental-support-for-svg: true;
		@include background-image(linear-gradient(top,  rgba(237,85,152,1) 0%,rgba(147,2,75,1) 100%));
	}
	@else if $gradient == magenta-round-dark {
		background-color: rgb(185,29,110);
		@include filter-gradient(#b91d6e, #4c0c2e, vertical);
		$experimental-support-for-svg: true;
		@include background-image(linear-gradient(top,  rgba(185,29,110,1) 0%,rgba(159,25,94,1) 29%,rgba(112,19,66,1) 63%,rgba(104,17,62,1) 71%,rgba(92,16,55,1) 80%,rgba(88,13,52,1) 89%,rgba(81,13,50,1) 91%,rgba(76,12,46,1) 100%));
	}
	@else if $gradient == magenta-round-light {
		background-color: rgb(237,85,152);
		@include filter-gradient(#ed5598, #b9026d, vertical);
		$experimental-support-for-svg: true;
		@include background-image(linear-gradient(top,  rgba(237,85,152,1) 0%,rgba(185,2,109,1) 100%));
	}
}

