@import "compass";
@import url(//fonts.googleapis.com/css?family=Open+Sans);

// General new Sprites
$icons-bg_marker_sort-spacing: 20px; // add spacing to this graphic in the sprite map
$icons-layout: smart; // initialize smart sprite map layout. default is vertical
@import "b_engagement/icons/*.png"; // create sprite map in this directory

@mixin b-engagement-sprite($name) {
	@include icons-sprite($name); // background url
	height: icons-sprite-height($name); // height of sprite
	width: icons-sprite-width($name); // width of sprite
	display: block;
}

@mixin h1 {
	font-size:24px; margin: 40px 0 0 0; color: $primary-color; font-weight: normal; font-family: 'Open Sans', sans-serif;
}

$primary-color: #da0670;

.biz-form-message-icon {
	position: absolute; left: -40px; top: 5px;
	&.is-doc { @include b-engagement-sprite(doc); }
	&.is-shield { @include b-engagement-sprite(shield); }
	&.is-risk { @include b-engagement-sprite(risk); }
}
.icon-secured { @include b-engagement-sprite(secured); float: right; margin: 0 55px 0 0; }

.b-pricing--success {
	.box-container-round { margin: 30px 0 0 0; width: auto; }
	.button1 { width: 250px; margin: 0 auto; }
}

// Engagement Page - Adam 
.text-underline { text-decoration: underline; }
.margin-bottom-20 { margin-bottom: 20px; }
.margin-left-20 { margin-left: 20px; }
.margin-right-20 { margin-right: 20px; }
.margin-top-20 { margin-top: 20px; }

// Placeholder 
.placeholderWrap{ position: relative;  display: inline-block;} 
.placeholderWrap label{ color: #AAA; position: absolute; top: 12px; left: 18px; pointer-events: none; display: block; }
.placeholder-focus label{ color: #666;}
.placeholder-changed label{ display: none; }

input[type="text"],
input[type="password"] { @include box-sizing(border-box);
	&:hover { @include box-shadow(#CDCDCD 2px 2px 10px); }
	&:focus { background-color: #EAEAEA;}
}

