/* old search box */
#search-result h1, #search-result h3.h1title { font-size: 18px; border-bottom: 1px #67c5c4 dashed; padding: 5px 0 10px 0; margin-bottom: 10px; }

.result-companies, .result-groups, .result-reports { margin-bottom: 25px; }

.result-groups ul, .result-reports ul, .result-comments ul { margin-bottom: 20px; }

.result-companies ul { margin-bottom: 5px; }

.result-companies ul li a, .result-groups ul li a, .result-reports ul li a, .result-comments ul li a { font-size: 16px; display: block; }

.result-groups ul li a, .result-reports ul li a, .result-comments ul li a { padding-bottom: 5px; display: block; }

.result-groups ul li p { font-size: 16px; line-height: 12px; }

.result-reports ul li p, .result-comments ul li p { line-height: 16px; padding-bottom: 0px; }

#search-result .s-date { padding-bottom: 5px; }

#search-result .s-date, #search-result .s-date a { font-size: 14px; color: #575757; }

#search-result .s-date a { display: inline; }

#search-result .s-date a:hover { color: #B8006C; }

#search-result .s-r-line { border-bottom: 1px dashed #67c5c4; display: block; padding-bottom: 20px; clear: both; }

fieldset.search { border: none; width: 218px; margin: 0 auto; -khtml-border-radius: 8px; -moz-border-radius: 8px; -webkit-border-radius: 8px; border-radius: 8px; }

.search-box input, .search-box button { border: none; float: left; -khtml-border-radius: 8px; -moz-border-radius: 8px; -webkit-border-radius: 8px; border-radius: 8px; }

.search-box input.box { color: #fff; font-size: 12px; text-shadow: 1px 1px 0 #cccccc; text-spaceing: 0.03em; font-weight: bold; width: 170px; height: 25px; padding: 0 5px; background: #BCD9D9; margin-right: 1px; line-height: 25px; font-family: Verdana, Geneva, sans-serif; -khtml-border-radius: 8px; -moz-border-radius: 8px; -webkit-border-radius: 8px; border-radius: 8px; }

.search-box input.box:focus { background: #557474; outline: none; color: #fff; }

.search-box .btn { width: 32px; height: 25px; cursor: pointer; text-indent: -9999px; background: #bcd9d9 url(css-img/icon-finder.png) no-repeat 3px 0; }

.search-box .btn:hover { background: #557474 url(css-img/icon-finder.png) no-repeat 3px -24px; }

.search-box .btn:focus { background: #557474 url(css-img/icon-finder.png) no-repeat 3px -23px; }

.box form select.style2 { margin-bottom: 14px; height: 30px; font-size: 18px; line-height: 28px; padding: 0 0 0 15px; width: 300px; border-radius: 5px; }
.box form label.style2 { margin: 0 0 6px 5px; }
.box form input.style2 { height: 24px; padding: 5px; font-size: 16px; text-indent: 5px; font-weight: bold; width: 95%; margin-bottom: 14px; border: 1px solid #cccccc; }
.box form input.style2, .box form textarea.style2 { color: #737373; border-radius: 24px; }
.box form input.style2:focus, .box form textarea.style2:focus { background: #efffff; color: #000; }
.box form textarea.style2 { font-size: 16px; padding: 10px; width: 95%; }

