#slideshow-wrapper { float:left; width:100%; margin-top:12px;}
#slidshow-right { height:200px;	width:791px; float:left; background-color:#FFF;	}
#slidshow-right img{ float:left;}
#slidshow-left { float:right; background-color:#1475A1; width:319px; height:200px;}
#slidshow-left li{ height:200px !important;}
#slidshow-left h1{ text-align:left;	margin:10px; font:bold 25px Arial; color:#fff;}
#slidshow-left p{ text-align:left; display:block; margin:10px; color:#fff; font-size:12px;}
#leftsidebar { float:left; margin-top:21px; width:140px; margin-right:10px;}
.textwidget { float: left; }
.textwidget li a{ display: block; padding: 5px 0 5px 5px; width: 275px; font-size:15px; font-family:verdana; color:#828282;}
.textwidget li{ float: left; margin-left: 10px; border-bottom:2px solid #EFEFEF;}
#content .post { width:580px; border-bottom:1px solid #ccc; padding:0px 0px 10px 0px; margin:0 0 10px 0; font-size:12px;}
#content .post .post_img .fl { float:left; padding:0px 10px 0 0; height:150px; width:250px; } 
#content .post .post_content { float:right; width:305px; } 
#content .post .post_content .entry-title { font-size:18px; padding-bottom:10px; height:auto;}
.post .entry-title {font-size:28px; padding-bottom:18px; /*color:#737373; */ text-shadow:1px 1px 0px #F2F9F9; word-spacing:-0.05em;}
#content .post .post_content .entry-title a {font-family: Arial, Helvetica, sans-serif; font-weight:bold; color:#737373; display:block; line-height:18px; text-decoration:none; word-spacing:-0.05em; text-shadow:1px 1px 0px #F2F9F9;} 
#content .post .entry-utility {clear:both; padding:10px 0 0 0; font-size:11px;}
#content .post .post_content .entry-title a:hover {  color:#B9006C; } 
#content .post .entry-meta { display:block; padding:0px 0px 6px 0px; }
#content .post .entry-meta span { }
#content .post .entry-content p { padding:8px 0px 0 0; margin-bottom:10px; }
#content .post .entry-content p a { font-weight:bold; }
#content .post .post_content .entry-summary p { margin:0 0 10px 0; padding:0px; font-size:12px; }
#content .post .post_content .entry-summary a {display:block;}
#content .post .post_content .entry-summary a:hover { color:#1475A1; }
.entry-content img {padding: 10px 0;}
.entry-content {line-height:18px;}
.permlink {padding-top:10px;}
/* #################### COMMENT #################### */
#comments-title {clear:both; padding-top:10px;}
#comments { font-size:11px; font-family:Verdana, Arial, Helvetica, sans-serif; padding-bottom:10px; }
	#comments p {line-height:20px; padding-bottom:5px;}
#respond { padding-top: 10px; font-size:11px; font-family:Verdana, Arial, Helvetica, sans-serif; margin-bottom:10px; }
.form-allowed-tags {display:none;}
.commentlist { color:#000; font-size:11px; font-family:Verdana, Arial, Helvetica, sans-serif; line-height:15px; }
.commentlist li.depth-1{ margin:30px 0px 0px 0px; }
.commentlist ul.children{ padding-left:10px; margin:30px 0px 0px 30px; border-left:2px #ccc solid;}
.comment-author img { float:left; margin:0 10px 0 0; border:1px solid #4F98B9; padding:1px; background: none repeat scroll 0 0 #FFFFFF; -moz-box-shadow: 1px 1px 1px 1px #d6d6d7; -webkit-box-shadow: 1px 1px 1px 1px  #d6d6d7; box-shadow: 1px 1px 1px 1px  #d6d6d7; }
.comment-author {clear:both; padding-bottom:5px;}
.comment-meta { padding:0 0 10px 0;}
.comment-meta a {color:#ccc; font-weight:bold; COLOR:#666;}
.comment-meta a:hover { color:#4F98B9; text-decoration:underline; }
.comment .reply a{ font-weight:bold; margin:0px 0px 10px 0; color:#666666; }
.comment .reply a:hover{ color:#4F98B9; text-decoration:underline; }
.comment .fn a{ font-weight:bold; margin:0px 0px 10px 0; }
#commentform p { margin:10px 0px; }
#commentform input, #commentform textarea{ display:block; border:1px solid #DADADA; font-size:11px; font-family:Verdana, Arial, Helvetica, sans-serif; padding:2px; width:400px; }
.form-submit #submit { cursor:pointer; font-weight:bold; width:150px; background:#4F98B9; color:#FFFFFF; border:1px solid #FFF; height:25px; display:block; }
.form-submit #submit:hover { background:#0C2048; color:#FFF; border:1px solid #FFF; }
#commentform label{ font-size:12px; margin:0 0 2px 0px; color: #4F98B9; font-weight:bold;  }
#commentform .required { color:red; }
#respond  { width:620px; margin:20px 0 0 0; padding:20px 0px; }
#respond a { font-weight:bold; margin:0px 0px 10px 0;}
