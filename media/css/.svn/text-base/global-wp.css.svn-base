#outerBox {	float:left;}
.wp-pagenavi { color: white; font-size: 14px; font-weight: normal; float:left; margin-top:8px;}
.wp-pagenavi a, .wp-pagenavi span { color: white; text-decoration: none; background-color: #a3a3a3;	-moz-border-radius: 2px; border-radius: 2px; border: 1px solid #FFF; padding: 3px 5px; margin: 2px;}
.wp-pagenavi a:hover, .wp-pagenavi span.current { color: white;	text-decoration: none;	background-color: #1475A1;	border:1px solid #1475A1 !important;}
.wp-pagenavi span.current {	font-weight: bold;}
.post_img {	width:250px; float:left; margin-top:10px;}
.post_img span{	height:150px; display:block;}
.nav-previous { float: left; width: 50%;}
.nav-next {	float: right; text-align: right; width: 50%;}
