#b-dash .acc-mngr .title { font-size:11px; float:left; line-height:16px; padding:0 10px 0 0; }
#b-dash .acc-mngr .img { height:30px; width:30px; float:left; padding-top:3px; }
#b-dash .acc-mngr .info { float:left; line-height:16px; font-size:11px; padding-left:5px; }
#b-dash .acc-mngr a { color:#000; }

/* advertising */
#b-dash .advertising-container { float:left; width:740px; }
#b-dash .advertising-container .overview { text-align:left; }
#b-dash .advertising-container .overview .current { font-size:22px; border-top:1px #ccc solid; margin-top:10px; padding-top:10px; }
.advertising-container .send-content, .advertising-container .inbox-msg-open { border:1px solid #ccc; padding:10px; margin:10px 0 10px 0; background:#fff; -moz-border-radius:10px; -webkit-border-radius:10px; border-radius:10px; -webkit-border-radius:10px; }
.advertising-container .in, .advertising-container .success { background:#EFFCE3; }
