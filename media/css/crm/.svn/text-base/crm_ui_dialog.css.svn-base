/* jQuery UI Dialog 1.8.17 */
.ui-dialog { position: absolute; padding: 0; width: 300px; overflow: hidden; border: 1px solid #ccc; box-shadow: 0 0 3px rgba(0, 0, 0, .20); -moz-box-shadow: 0 0 3px rgba(0, 0, 0, .20); -webkit-box-shadow: 0 0 3px rgba(0, 0, 0, .20); background: url('/media/images/crm/ui-dialog/bgnoise.png'); }
.ui-dialog .ui-dialog-titlebar { padding: .4em 1em; position: relative; background: #494949; border: none; -moz-border-radius: 0; -webkit-border-radius: 0; border-radius: 0; }
.ui-dialog .ui-dialog-title { float: left; margin: .1em 16px .1em 0; color: #fff; text-shadow: 1px 1px 1px rgba(0, 0, 0, .25); }
.ui-dialog .ui-dialog-titlebar-close { position: absolute; right: .3em; top: 50%; width: 19px; margin: -10px 0 0 0; padding: 1px; height: 18px; }
.ui-dialog .ui-dialog-titlebar-close span { display: block; margin: 1px; }
.ui-dialog .ui-dialog-titlebar-close:hover, .ui-dialog .ui-dialog-titlebar-close:focus { padding: 0; }
.ui-dialog .ui-dialog-content { position: relative; border: 0; padding: 1em 1em .5em 1em; background: none; overflow: auto; zoom: 1; }
/* UI Text Area */
.ui-dialog .ui-dialog-content input:focus, .ui-dialog .ui-dialog-content textarea:focus, select:focus { border: 1px solid #ccc; -moz-box-shadow: 0 0 3px #ccc, 0 1px 2px rgba(0, 0, 0, .15) inset; -webkit-box-shadow: 0 0 3px #ccc, 0 1px 2px rgba(0, 0, 0, .15) inset; box-shadow: 0 0 3px #ccc, 0 1px 2px rgba(0, 0, 0, .15) inset; }
.ui-dialog .ui-dialog-buttonpane { background: #f9f9f9; text-align: left; border-width: 1px 0 0 0; background-image: none; margin: .5em 0 0 0; padding: .3em 1em .5em .4em; }
.ui-dialog .ui-dialog-buttonpane .ui-dialog-buttonset { float: right; }
/* UI Dialog Buttons */
.ui-dialog .ui-dialog-buttonpane button { background-repeat: repeat-x; background-position: 0 0; font-weight: bold; text-align: center; text-decoration: none; line-height: normal; color: #FFF; display: inline-block; margin: .5em .4em .5em 0; *overflow: visible; position: relative; border: none; border-width: 1px; border-style: solid; cursor: pointer; -moz-border-radius: 4px; -webkit-border-radius: 4px; border-radius: 4px; text-shadow: 1px 1px 1px rgba(0, 0, 0, .25); -moz-box-shadow: 1px 1px 1px rgba(0, 0, 0, .25); -webkit-box-shadow: 1px 1px 1px rgba(0, 0, 0, .25); box-shadow: 1px 1px 1px rgba(0, 0, 0, .25); -moz-box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.3); -webkit-box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.3); box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.3); }
.ui-dialog .ui-dialog-buttonpane button, .ui-dialog .ui-dialog-buttonpane button:visited, .progress-bar.primary .bar { background: #00a8a8; background:-moz-linear-gradient(top, #95d6d7 0%, #51c3c6 2%, #00a8a8 100%); /* FF3.6+ */ background:-webkit-gradient(linear, left top, left bottom, color-stop(0%, #95d6d7), color-stop(2%, #51c3c6), color-stop(100%, #00a8a8)); /* Chrome,Safari4+ */ background:-webkit-linear-gradient(top, #95d6d7 0%, #51c3c6 2%, #00a8a8 100%); /* Chrome10+,Safari5.1+ */ background:-o-linear-gradient(top, #95d6d7 0%, #51c3c6 2%, #00a8a8 100%); /* Opera11.10+ */ background:-ms-linear-gradient(top, #95d6d7 0%, #51c3c6 2%, #00a8a8 100%); /* IE10+ */ background:linear-gradient(top, #95d6d7 0%, #51c3c6 2%, #00a8a8 100%); /* W3C */  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#51c3c6', endColorstr='#00a8a8'); -ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr='#51c3c6', endColorstr='#00a8a8')"; color: #FFF; border-color: #019090; -moz-box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.3); -webkit-box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.3); box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.3); }
.ui-dialog .ui-dialog-buttonpane button:hover { background: #019090; background:-moz-linear-gradient(top, #90e1e2 0%, #60c9cc 2%, #019090 100%); /* FF3.6+ */ background:-webkit-gradient(linear, left top, left bottom, color-stop(0%, #90e1e2), color-stop(2%, #60c9cc), color-stop(100%, #019090)); /* Chrome,Safari4+ */ background:-webkit-linear-gradient(top, #90e1e2 0%, #60c9cc 2%, #019090 100%); /* Chrome10+,Safari5.1+ */ background:-o-linear-gradient(top, #90e1e2 0%, #60c9cc 2%, #019090 100%); /* Opera11.10+ */ background:-ms-linear-gradient(top, #90e1e2 0%, #60c9cc 2%, #019090 100%); /* IE10+ */ background:linear-gradient(top, #90e1e2 0%, #60c9cc 2%, #019090 100%); /* W3C */  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#60c9cc', endColorstr='#019090');
-ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr='#60c9cc', endColorstr='#019090')"; }
.ui-dialog .ui-dialog-buttonpane button:active { background: #00a8a8; background:-moz-linear-gradient(top, #00a8a8 2%, #51c3c6 100%); /* FF3.6+ */ background:-webkit-gradient(linear, left top, left bottom, color-stop(2%, #00a8a8), color-stop(100%, #51c3c6)); /* Chrome,Safari4+ */ background:-webkit-linear-gradient(top, #00a8a8 2%, #51c3c6 100%); /* Chrome10+,Safari5.1+ */ background:-o-linear-gradient(top, #00a8a8 2%, #51c3c6 100%); /* Opera11.10+ */ background:-ms-linear-gradient(top, #00a8a8 2%, #51c3c6 100%); /* IE10+ */ background:linear-gradient(top, #00a8a8 2%, #51c3c6 100%); /* W3C */  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#00a8a8', endColorstr='#51c3c6'); -ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr='#51c3c6', endColorstr='#00a8a8')"; color: #FFF; border-color: #019090; }

.ui-dialog .ui-resizable-se { width: 14px; height: 14px; right: 3px; bottom: 3px; }
.ui-draggable .ui-dialog-titlebar { cursor: move; }