#landing{
	.landing-content{padding: 0 100px;background: #fff;  height:auto;padding:41px 50px;width:640px;margin:0 auto;
		h1{color:#000; font-size:24px}
		.landing-heading-1{background: url(css-img/landing-heading-l.png)no-repeat left center #fff;  font-size: 30px; padding: 0 0 0 30px;color:#d9007f; margin: 10px 0;		}
		#category-select{background:transparent; padding: 20px 0;height: 40px; border-top:1px #ccc solid; border-bottom:1px #ccc solid;margin:0 0 10px 0;
		  label{display:block;float:left;width:197px;height: 40px;line-height: 40px;}
			.landing-main-btns{float:right; margin:0;width:410px; 
			  input{width:133px;margin:0 }
			}
		}	
		textarea {font-size: 16px;padding: 10px;width: 95%;}  
	  #subform {width:auto;
		  div{width:100%;}
		  .all{background:transparent;
				.col-2a,.col-2b{ background:transparent;width:auto;	padding:0;			
				  .details{
						label{display:block;float:left;width:190px;height: 40px;line-height: 40px;}
						input,textarea,select{width:405px;float:right;display:block;padding:10px 0 5px 10px}
					  select{padding:0;}
						.half{width:100%;margin:0;}
						.general-form { width: auto;}
					}
					input#btn-next1{float:right;}	
				}
	    } /* end all */
		}/* end subform */
		.landing-footer{border-bottom:1px #ccc solid}
	}/* end landing-content */
}/* end #landing */