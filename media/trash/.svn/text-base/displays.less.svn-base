@import url('variables.less');

.sub-menu {
	height: 45px; clear:both; text-shadow: 1px 1px #cfeef2;
	background: #aac3c7; /* Old browsers */
	/* IE9 SVG, needs conditional override of 'filter' to 'none' */
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(top,  #aac3c7 0%, #c1dee2 11%, #c1dee2 94%, #b8d3d8 97%, #d5f5fa 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#aac3c7), color-stop(11%,#c1dee2), color-stop(94%,#c1dee2), color-stop(97%,#b8d3d8), color-stop(100%,#d5f5fa)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  #aac3c7 0%,#c1dee2 11%,#c1dee2 94%,#b8d3d8 97%,#d5f5fa 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  #aac3c7 0%,#c1dee2 11%,#c1dee2 94%,#b8d3d8 97%,#d5f5fa 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  #aac3c7 0%,#c1dee2 11%,#c1dee2 94%,#b8d3d8 97%,#d5f5fa 100%); /* IE10+ */
	background: linear-gradient(top,  #aac3c7 0%,#c1dee2 11%,#c1dee2 94%,#b8d3d8 97%,#d5f5fa 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#aac3c7', endColorstr='#d5f5fa',GradientType=0 ); /* IE6-8 */
	.container {width: 900px; margin: 0 auto; position: relative; height: 45px;}
	.menu {position: absolute; bottom: 0; margin: 0 0 0 -1px;
		.arrow-down {position: absolute; top:16px; right: 15px; width: 0; height: 0; border-left: 6px solid transparent; border-right: 6px solid transparent; border-top: 6px solid #666;}
	}
	.menu>li {margin: 0 15px 0 0; display: inline-block; float: left; font-weight: bold; height: 35px; line-height: 35px}
	.menu>li.active {background: #f0fffe; border-radius: 4px 4px 0 0; box-shadow: 0 -2px 2px 0 #98cace; -webkit-box-shadow: 0 -2px 2px 0 #98cace; text-shadow: 1px 1px #fff;}
	.menu>li a { position: relative; padding: 12px 35px; color: #666;}
	.menu>li a:hover {text-shadow: 1px 1px #fff;}
	.dropdown-menu {z-index:9999; width: 600px; box-shadow: 3px 3px 3px #999; -webkit-box-shadow: 3px 3px 3px #999; border:1px solid #c1dee2; border-top: 0; border-radius: 0 0 8px 8px; position: absolute; top: 34px; left: 0; margin: 0 0 0 -1px;
		background: #f0fffe; /* Old browsers */
		/* IE9 SVG, needs conditional override of 'filter' to 'none' */
		background: url(data:image/svg+xml;base64,);
		background: -moz-linear-gradient(top,  #f0fffe 0%, #ffffff 100%); /* FF3.6+ */
		background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f0fffe), color-stop(100%,#ffffff)); /* Chrome,Safari4+ */
		background: -webkit-linear-gradient(top,  #f0fffe 0%,#ffffff 100%); /* Chrome10+,Safari5.1+ */
		background: -o-linear-gradient(top,  #f0fffe 0%,#ffffff 100%); /* Opera 11.10+ */
		background: -ms-linear-gradient(top,  #f0fffe 0%,#ffffff 100%); /* IE10+ */
		background: linear-gradient(top,  #f0fffe 0%,#ffffff 100%); /* W3C */
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f0fffe', endColorstr='#ffffff',GradientType=0 ); /* IE6-8 */
		.col1, .col2, col3 {float:left; width: 32%; margin: 10px 0 0 15px;
			li {
				float:left; clear:both; height: 26px; line-height: 26px; margin: 0 0 15px 0; width: 100%\9; text-align: left\9;
				a {padding: 5px 18px; color: #666;}
				a:hover {color:#B9006C;}
			}
			li.active {background: #fff; box-shadow: inset 2px 2px 2px #ddd; -webkit-box-shadow: inset 2px 2px 2px #ddd; border-radius: 16px; border:1px solid #d3e0df;}
		}
	}

	.filters {
		padding:20px;
		h4 {font-size:14px; color:#666; padding:0px 0 0px 0;}
		div {float:left;}
		label {font-weight:normal; margin:0 5px;}
		.btn-s3 {float:right; margin:10px 10px 0 0;}
		a.browse-close {width:5px; height:5px; float:right; padding:3px 7px 10px 5px; margin:-10px 0 0 0; background:#ccc; line-height:10px;}
		.type {width:250px; height:80px; margin-right:15px; padding-right:15px; border-right:1px solid #ccc;}
		.country {
			width:100px; height:80px;
			select {width:127px;}
		}
		.has {width:150px; height:80px; margin-right:15px; padding-right:15px; border-right:1px solid #ccc;}
		.row {
			width:100%; margin-bottom:20px;
			input {width:90px; padding:5px; text-align:center;}
			.submitted{width:280px; margin-right:30px;}
		}
	}
}


