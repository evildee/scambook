@import url('variables.less');

.header {
	width: 890px; margin: 0 auto; position: relative;
	.bar1 {
		height: 47px;
		.logo {
			padding: 12px 0 0 0; float: left;
			a { display: block; background: url(css-img/logo.png) no-repeat; width: 144px; height: 21px; text-indent: -9999px; }
		}
		.tagline {
			margin: 10px 0 0 10px; float: left;
			li {  }
		}
		.drop-menu {
			float: right;
			li {
				display: inline;
				a {
					display: inline-block; padding: 10px 20px 0 20px; border-right: 1px solid #bcdaeb; box-shadow: 1px 0 0 #d1eff7; -webkit-box-shadow: 1px 0 0 #d1eff7; color: #333; text-shadow: 1px 1px 0 #f6faff; float: left;
					.icon.us-flag {
						text-indent: 0; background-position: -55px -364px; margin: 0 10px 0 0;
					}
					.triangle-icon { width: 15px; height: 15px; display: block; margin: 0 0 0 5px; float: right; background: url(css-img/jq-ui/ui-icons_222222_256x240.png) -64px -18px no-repeat; }	
				}
			}
			li.first-child {
				a { border-left: 1px solid #bcdaeb; }
			}
			li.active {
				a {
					background: #b9ccda; /* Old browsers */
					/* IE9 SVG, needs conditional override of 'filter' to 'none' */
					background: url(data:image/svg+xml;base64,);
					background: -moz-linear-gradient(top,  #b9ccda 0%, #d0e5f3 100%); /* FF3.6+ */
					background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#b9ccda), color-stop(100%,#d0e5f3)); /* Chrome,Safari4+ */
					background: -webkit-linear-gradient(top,  #b9ccda 0%,#d0e5f3 100%); /* Chrome10+,Safari5.1+ */
					background: -o-linear-gradient(top,  #b9ccda 0%,#d0e5f3 100%); /* Opera 11.10+ */
					background: -ms-linear-gradient(top,  #b9ccda 0%,#d0e5f3 100%); /* IE10+ */
					background: linear-gradient(to bottom,  #b9ccda 0%,#d0e5f3 100%); /* W3C */
					filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#b9ccda', endColorstr='#d0e5f3',GradientType=0 ); /* IE6-8 */
					
				}
			}
		}
		.drop-panel {
			position: absolute; background: #fff; top: 47px; right: 0; z-index: 100; padding: 20px; width: 270px; border-radius: 0 0 5px 5px; box-shadow: 8px 8px 18px rgba(0,0,0,.1), -8px 8px 18px rgba(0,0,0,.1); -webkit-box-shadow: 8px 8px 18px rgba(0,0,0,.1), -8px 8px 18px rgba(0,0,0,.1);
			.drop-shadow {
				width: 100%; height: 18px; position: absolute; top: 0; left: 0; background: #cde1f1; /* Old browsers */
				/* IE9 SVG, needs conditional override of 'filter' to 'none' */
				background: url(data:image/svg+xml;base64,);
				background: -moz-linear-gradient(top,  #cde1f1 0%, #f7f7f7 41%, #ffffff 99%); /* FF3.6+ */
				background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#cde1f1), color-stop(41%,#f7f7f7), color-stop(99%,#ffffff)); /* Chrome,Safari4+ */
				background: -webkit-linear-gradient(top,  #cde1f1 0%,#f7f7f7 41%,#ffffff 99%); /* Chrome10+,Safari5.1+ */
				background: -o-linear-gradient(top,  #cde1f1 0%,#f7f7f7 41%,#ffffff 99%); /* Opera 11.10+ */
				background: -ms-linear-gradient(top,  #cde1f1 0%,#f7f7f7 41%,#ffffff 99%); /* IE10+ */
				background: linear-gradient(to bottom,  #cde1f1 0%,#f7f7f7 41%,#ffffff 99%); /* W3C */
				filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#cde1f1', endColorstr='#ffffff',GradientType=0 ); /* IE6-8 */
			}
			a {
				.icon.spanish-flag	{ text-indent: 0; background-position: -55px -364px; margin: 0 10px 0 0; }
			}
			.form-general {
				width: 270px;
				li { border: 0; }
				label { width: 90px; padding: 5px 0; }
				.input-panel { padding: 5px 0; margin-left: 100px; border: 0; }
				.button {
					.button1 { padding: 5px 15px; float: right; }
				}
			}
			.button1.fb, .button1.gf {
				width: 100%; margin: 10px 0 0 0; position: relative; text-align: left;
				.icon { margin: 0 15px 0 -15px; }
			}
			.button1.fb {
				.icon { background-position: -55px -305px; }
			}
			.button1.gf {
				.icon { background-position: -55px -334px; }
			}
			.button1.fb {
				color: #fff !important; text-shadow: 1px 1px 0 #000;
				background: #5e81be; /* Old browsers */
				/* IE9 SVG, needs conditional override of 'filter' to 'none' */
				background: url(data:image/svg+xml;base64,);
				background: -moz-linear-gradient(top,  #5e81be 0%, #344c7d 100%); /* FF3.6+ */
				background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#5e81be), color-stop(100%,#344c7d)); /* Chrome,Safari4+ */
				background: -webkit-linear-gradient(top,  #5e81be 0%,#344c7d 100%); /* Chrome10+,Safari5.1+ */
				background: -o-linear-gradient(top,  #5e81be 0%,#344c7d 100%); /* Opera 11.10+ */
				background: -ms-linear-gradient(top,  #5e81be 0%,#344c7d 100%); /* IE10+ */
				background: linear-gradient(to bottom,  #5e81be 0%,#344c7d 100%); /* W3C */
				filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#5e81be', endColorstr='#344c7d',GradientType=0 ); /* IE6-8 */
			}
			.button1.gf {
				color: #666 !important; text-shadow: 1px 1px 0 #fff;
				background: #ffffff; /* Old browsers */
				/* IE9 SVG, needs conditional override of 'filter' to 'none' */
				background: url(data:image/svg+xml;base64,);
				background: -moz-linear-gradient(top,  #ffffff 0%, #f2f2f2 100%); /* FF3.6+ */
				background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ffffff), color-stop(100%,#f2f2f2)); /* Chrome,Safari4+ */
				background: -webkit-linear-gradient(top,  #ffffff 0%,#f2f2f2 100%); /* Chrome10+,Safari5.1+ */
				background: -o-linear-gradient(top,  #ffffff 0%,#f2f2f2 100%); /* Opera 11.10+ */
				background: -ms-linear-gradient(top,  #ffffff 0%,#f2f2f2 100%); /* IE10+ */
				background: linear-gradient(to bottom,  #ffffff 0%,#f2f2f2 100%); /* W3C */
				filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#f2f2f2',GradientType=0 ); /* IE6-8 */
			}
		}
	}
	.bar2{
		.menu {
			li {
				a { float: left; display: block; padding: 15px 25px; color: #fff; text-shadow: 1px 1px 0 #000; border-right: 1px solid #8a2c6c; box-shadow: 1px 0 0 #ba3b92; }
			}
			li.first-child {
				a { padding-left: 0; }
			}
		}
	}
	
}


